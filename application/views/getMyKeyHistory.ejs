<body align="center">
    <% include ./header %>
    <div>
        <h2>제품 키 히스토리 조회</h2>
        <table id="tableData" align="center">
            <% data.data.forEach(function (item, index) { %>
            <center>    
                <tr>
                    <td>TX ID</td>
                    <td colspan='4'>DATA</td>
                    <td>TIMESTAMP</td>
                </tr>
                <tr>
                    <td><%= item.TxId %></td>
                    <td>{ poomId: <%= item.Value.poomId %>, </td></a>
                    <td>poomKey: <%= item.Value.poomKey %>, </td>
                    <td>poomOwner: <%= item.Value.poomOwner %>,</td>
                    <td>poomValidity: <%= item.Value.poomValidity %> }</td>
                    <td><%= item.Timestamp %></td>
                </tr>
            </center>
            <% }); %> 
        </table>
    </div>
    <a href='/'><button type="submit" id="submit" class="btn btn-primary">홈으로</button></a>
</body>

</html>